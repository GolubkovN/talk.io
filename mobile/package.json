{
  "name": "talkio",
  "main": "index.ts",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reinstall": "rm -rf node_modules && bun install",
    "reset-project": "node ./scripts/reset-project.js",
    "start:dev": "dotenv -e env/dev.env -- expo start",
    "start:uat": "dotenv -e env/uat.env -- expo start",
    "start:prod": "dotenv -e env/prod.env -- expo start",
    "ios:dev": "dotenv -e env/dev.env -- expo run:ios",
    "ios:uat": "dotenv -e env/uat.env -- expo run:ios",
    "ios:prod": "dotenv -e env/prod.env -- expo run:ios",
    "android:dev": "dotenv -e env/dev.env -- expo run:android",
    "android:uat": "dotenv -e env/uat.env -- expo run:android",
    "android:prod": "dotenv -e env/prod.env -- expo run:android",
    "web": "expo start --web",
    "lint": "expo lint",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@bottom-tabs/react-navigation": "^1.1.0",
    "@clerk/clerk-expo": "^2.19.22",
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo-google-fonts/lobster": "^0.4.1",
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@reduxjs/toolkit": "^2.11.2",
    "date-fns": "^4.1.0",
    "expo": "~54.0.31",
    "expo-blur": "^15.0.8",
    "expo-build-properties": "~1.0.10",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-font": "^14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-secure-store": "^15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "pressto": "^0.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-bottom-tabs": "^1.1.0",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-nitro-modules": "^0.33.7",
    "react-native-reanimated": "^4.2.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-unistyles": "^3.0.22",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "^0.7.2",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "packageManager": "bun@1.3.8",
  "private": true
}
